{
  "name": "material-multi-picker",
  "version": "0.1.0",
  "description": "A multiselect component that loads dynamic previews",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/atropos-tech/material-multi-picker.git"
  },
  "scripts": {
    "test": "jest --watch --coverage",
    "sandbox": "webpack-dev-server --config webpack.config.js --hot --inline --color --progress --open --mode development",
    "transpile": "babel src --out-dir lib --ignore \"src/**/*.spec.js\",\"src/sandbox/**/*",
    "lint": "eslint src",
    "start": "npm run sandbox"
  },
  "keywords": [
    "component"
  ],
  "author": "Duncan Thacker (duncan.thacker@atropos.co.uk)",
  "license": "ISC",
  "dependencies": {
    "create-react-class": "^15.6.3",
    "downshift": "^3.2.0",
    "keycode": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@material-ui/core": "^3.9.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.12.1",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jest": "^22.2.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "jest": "^23.6.0",
    "jest-environment-enzyme": "^7.0.1",
    "jest-enzyme": "^7.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.8.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "peerDependencies": {
    "react": "^15.6"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "jest-enzyme",
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/sandbox/"
    ]
  }
}
